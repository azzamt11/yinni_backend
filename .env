# ---- Auth ----
AUTH_HTTP_PORT=8000
AUTH_GRPC_PORT=9000

# ---- User ----
USER_HTTP_PORT=8001
USER_GRPC_PORT=9001

# ---- Product ----
PRODUCT_HTTP_PORT=8002
PRODUCT_GRPC_PORT=9002

# ---- JWT ----
JWT_SECRET=super-secret-key
JWT_EXPIRE=3600

# ---- Database ----
MYSQL_ROOT_PASSWORD=root
MYSQL_DATABASE=yinni_db
